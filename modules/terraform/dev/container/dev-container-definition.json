[
  {
    "name": "express",
    "image": "354221491810.dkr.ecr.ap-northeast-1.amazonaws.com/kurakichi:dev-express",
    "essential": true,
    "memory": 512,
    "cpu": 256,
    "memoryReservation": 128,
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 80,
        "protocol": "tcp"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/kurakichi",
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "express"
      }
    }
  },
  {
    "name": "redis",
    "image": "354221491810.dkr.ecr.ap-northeast-1.amazonaws.com/kurakichi:dev-redis",
    "cpu": 128,
    "memory": 256,
    "portMappings": [
      {
        "containerPort": 6379,
        "hostPort": 6379,
        "protocol": "tcp"
      }
    ]
  },
  {
    "name": "postgres",
    "image": "354221491810.dkr.ecr.ap-northeast-1.amazonaws.com/kurakichi:dev-psql",
    "cpu": 128,
    "memory": 256,
    "portMappings": [
      {
        "containerPort": 5432,
        "hostPort": 5432,
        "protocol": "tcp"
      }
    ],
    "secrets": [
      {
        "name": "POSTGRES_USER",
        "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:354221491810:secret:kurakichi_secrets-qKUrm4"
      },
      {
        "name": "POSTGRES_DB",
        "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:354221491810:secret:kurakichi_secrets-qKUrm4"
      },
      {
        "name": "POSTGRES_PASSWORD",
        "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:354221491810:secret:kurakichi_secrets-qKUrm4"
      }
    ]
  }
]
