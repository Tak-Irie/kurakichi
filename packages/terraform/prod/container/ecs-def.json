[
  {
    "name": "kurakichi",
    "image": "none",
    "essential": true,
    "memory": 384,
    "memoryReservation": 128,
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 80,
        "protocol": "tcp"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/kurakichi",
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "express"
      }
    }
  }
]
